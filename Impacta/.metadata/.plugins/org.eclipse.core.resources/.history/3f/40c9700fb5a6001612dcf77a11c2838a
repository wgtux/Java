
public class TesteConta {

	public static void main(String[] args) {
	
		Conta c1 = new Conta();
		c1.nome = "Weslen Gomes";
		c1.num = 2016;
		c1.saldo = 2500.00;
		
		System.out.println("Dados da Conta: ");
		System.out.println(c1.nome);
		System.out.println(c1.num);
		System.out.println(c1.saldo);
	}

}
